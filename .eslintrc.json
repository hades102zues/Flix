{
  "parser": "babel-eslint",
  "extends": ["plugin:react/recommended", "eslint:recommended"],

  "env": {
    "jest": true,
    "browser": true,
    "es6": true
  },
  "plugins": ["react"],
  "rules": {
    "no-use-before-define": "off",
    "react/jsx-filename-extension": "off",
    "react/prop-types": "off",
    "comma-dangle": "off"
  }
}
